	<div class="row-fluid">
		<div id="wrapper">
			<div id="columns">
				<% for(i = 0; i < imageList.length; i++) {
					var divID = "id=\'block" + i + "\'" %>
					<div class="pin back-image" <%=divID%>>
						<img src="<%=imageList[i].image_path%>" alt="<%=imageList[i].alt_text%>">
						<button class="btn btn-primary btn-mini float-left" id="like<%=i%>" type="button">Likes: <%=imageList[i].numLikes%></button>
					</div>
				<% } %>
			</div>
		</div>
	</div><!-- images showcase -->
